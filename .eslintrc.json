{
  "parser": "babel-eslint",
  "extends": ["airbnb", "plugin:prettier/recommended"],
  "rules": {
    "no-use-before-define": 0,
    "no-nested-ternary": 0,
    "no-restricted-syntax": 0,
    "strict": 0,
    "react/prop-types": 0
  },
  "overrides": [
    {
      "files": ["*.js"],
      "rules": {
        "import/no-extraneous-dependencies": 0
      }
    }
  ]
}
